<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<BorderPane maxHeight="400.0" maxWidth="600.0" minHeight="397.0" minWidth="481.0" prefHeight="397.0" prefWidth="481.0" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.uh.tech.cis3368.testingnancy.testingnancy.Controllers.FlavorViewController">
    <center>
        <TableView fx:id="FlavorTable" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <columns>
                <TableColumn fx:id="FlavIdCol" prefWidth="75.0" text="Flavor Id" />
                <TableColumn fx:id="FlavStatCol" prefWidth="75.0" text="Flavor Status" />
                <TableColumn fx:id="FlavTypeCol" prefWidth="75.0" text="Flavor Type" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
    </center>
    <bottom>
        <Pane prefHeight="63.0" prefWidth="481.0" style="-fx-background-color: #F5F5F5;" BorderPane.alignment="CENTER">
            <children>
                <JFXButton fx:id="flavAddBtn" layoutX="136.0" layoutY="15.0" onAction="#addFlavEvent" prefHeight="29.0" prefWidth="70.0" stylesheets="@dashboardcss.css" text="Add" />
                <JFXButton fx:id="flavEditBtn" layoutX="206.0" layoutY="15.0" onAction="#editFlavEvent" prefHeight="29.0" prefWidth="70.0" stylesheets="@dashboardcss.css" text="Edit" />
                <JFXButton fx:id="flavDeleteBtn" layoutX="276.0" layoutY="15.0" onAction="#deleteFlavEvent" prefHeight="34.0" prefWidth="75.0" stylesheets="@dashboardcss.css" text="Delete" />
            </children>
        </Pane>
    </bottom>
</BorderPane>

